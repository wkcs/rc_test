                    },
                    {
                        "text": "break rc_test_loop",    
                        "description": "halt",
                        "ignoreFailures": false
                    },
                ],
                "launchCompleteCommand": "None",
                "program": "${workspaceRoot}\\rc11x_test.elf",
                "args": [
                    "target remote localhost:3333"
